{{>layouts/header}}
<table class="table">
    <thead>
      <tr>
        <th scope="col">Id</th>
        <th scope="col">Title</th>
        <th scope="col">Content</th>
      </tr>
    </thead>
    <tbody>
      {{#article}} {{!모델에 등록한 article Entity 출력}}
      <tr>
        <th>{{id}}</th>
        <td>{{title}}</td>
        <td>{{content}}</td>
      </tr>
      {{/article}}
    </tbody>
  </table>
  <a href="/articles/{{article.id}}/edit" class="btn btn-priamary">Edit</a>
  <a href="/articles">Go to Article List</a>
{{>layouts/footer}}