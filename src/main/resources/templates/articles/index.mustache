{{>layouts/header}}
<h1>Articles</h1>
<table class="table">
    <thead>
      <tr>
        <th scope="col">Id</th>
        <th scope="col">Title</th>
        <th scope="col">Content</th>
      </tr>
    </thead>
    <tbody>
      {{#articleList}} {{!모델에 등록한 articleList Entity 출력}}
      <tr>
        <th>{{id}}</th>
        <td><a href="/articles/{{id}}">{{title}}</a></td>
        <td>{{content}}</td>
      </tr>
      {{/articleList}}
    </tbody>
  </table>
  <a href="/" class="btn btn-sm btn-outline-success">Go to Main</a>
  <a href="/articles/new" class="btn btn-primary">New Article</a>
{{>layouts/footer}}